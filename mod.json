{
	"geode": "4.7.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "sawblade.completion_tracker",
	"name": "Completion Tracker",
	"version": "v1.0.1",
	"developer": "Sawblade",
	"description": "Tracks all of your level completions",
	"links": {
		"source": "https://github.com/SawBlade21/Completion-Tracker"
	},
	"dependencies": {
		"cvolton.level-id-api": {
			"version": ">=v1.2.2",
			"importance": "required"
		},
		"geode.node-ids": {
			"version": ">=v1.21.1",
			"importance": "required"
		}
	},
	"resources": {
        "sprites": [
            "resources/jumps.png",
			"resources/rebeat_btn_01.png",
			"resources/rebeat_btn_02.png",
			"resources/paste_icon_btn.png",
			"resources/auto_btn.png",
			"resources/garage_btn.png",
			"resources/GJ_coinsIcon2_gray_001.png",
			"resources/sort_btn.png",
			"resources/thumb1.png",
			"resources/thumb2.png",
			"resources/thumb3.png"
        ]
    },
	"settings": {
		"time-format": {
			"name": "Time Format",
			"description": "Whether to use the 12-hour or 24-hour time format. Times will be automatically converted.",
			"type": "string",
			"default": "12-hour",
			"one-of": ["12-hour", "24-hour"]
		},
		"endscreen-button": {
			"name": "Endscreen Button",
			"description": "Adds the completion popup button to the endscreen.",
			"type": "bool",
			"default": false
		},
		"autofill": {
			"name": "Autofill",
			"description": "Automatically autofill data from the level when opening the 'Create Completion' menu.",
			"type": "bool",
			"default": false
		}
	}
}
